<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003"> 
  <Import Project="BuildMonoFramework.props" />
  <Import Project="BuildNetFramework.props" />
  <PropertyGroup>
    <VersionMajor>0</VersionMajor>
    <VersionMinor>1</VersionMinor>
    <VersionPatch>1</VersionPatch>
    <VersionSuffix></VersionSuffix>
    <VersionPrefix>$(VersionMajor).$(VersionMinor).$(VersionPatch)</VersionPrefix>
    
    <AssemblyVersion>$(VersionMajor).$(VersionMinor).0.0</AssemblyVersion>
    <FileVersion>$(VersionPrefix).0</FileVersion>

    <Product>Anonimize</Product>
    <Company>Joel Francisco</Company>
    <Copyright>Licensed under LGPL-2.1</Copyright>
    <Authors>Joel Francisco</Authors>
    <NeutralLanguage>en-UK</NeutralLanguage>

    <PackageProjectUrl>https://github.com/jodufra/anonimize</PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/jodufra/anonimize/master/logo.png</PackageIconUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://raw.githubusercontent.com/jodufra/anonimize/master/LICENSE.txt</PackageLicenseUrl>
    <PackageReleaseNotes>https://github.com/jodufra/anonimize/blob/$(VersionPrefix)/releasenotes.txt</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/jodufra/anonimize.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    
    <DisableImplicitPackageTargetFallback>True</DisableImplicitPackageTargetFallback>
  </PropertyGroup>
</Project>
